num1=float(input('Digite o primeiro número:'))
num2=float(input('Digite o segundo número:'))
print('1-Soma\n2-Subtração\n3-Multiplicação\n4-Divisão')
resp=input('Digite o operador:')
if resp=='adição':
    resp=num1+num2
    print(resp)
elif resp=='subtração':
    resp=num1-num2
    print(resp)
elif resp=='multiplicação':
    resp=num1*num2
    print(resp)
elif resp=='divisão':
    resp=num1/num2 
    print(resp)
