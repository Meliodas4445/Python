num=int(input('Digite o número:'))
divisao=input('Digite se quer multiplicação ou divisão:')
contador=1#Variáveis são num,divisao e contador
while contador<=10: #Contador vai multiplicar ou divvidir o num 10 vezes
     if divisao=='multiplicação':#Multiplicação
      result=num*contador
      print(num," X ",contador," = ",result,"\n")
      contador=contador+1
     elif divisao=='divisão': #divisão
      result=num/contador
      print(num," : ",contador," = ",result,"\n")
      contador=contador+1
    
